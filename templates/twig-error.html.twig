    <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.5.1/languages/twig.min.js" integrity="sha512-5VL1WJAySb8qYzsN0rDLo8VjL9LY0rvmeTffogQNPT8gVBH5O90kGbRQmsrFSPA6Jgr/WJmbuiGO3AvdZdaA7g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script>hljs.highlightAll();</script>
</head>
<body style="font-family: sans-serif">
    <h1>Error</h1>
    <p>{{ message }}</p>
    <p>{{ name }} : {{ line }}</p>
    <pre><code class=\"language-twig\">{{ code }}</code></pre>
</body>
</html>